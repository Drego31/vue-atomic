{
  "name": "@dbetka/vue-atomic",
  "version": "0.0.5",
  "description": "Atomic Design widgets for Vue.js",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "",
    "build-plugin": "npx bili --bundle-node-modules",
    "build-full-css": "node scripts/bundle-full-css.js --input src/index.sass --output dist",
    "build-wigets-css": "node scripts/bundle-wigets-css.js --input src/styles/components --output dist --base-file src/base-file.sass",
    "build": "npm run build-full-css & npm run build-wigets-css & npm run build-plugin"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbetka/vue-atomic.git"
  },
  "author": "dbetka",
  "bugs": {
    "url": "https://github.com/dbetka/vue-atomic/issues"
  },
  "homepage": "https://github.com/dbetka/vue-atomic#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "bili": "^4.9.0",
    "directory-tree": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "glob": "^7.1.6",
    "node-sass": "^4.14.1",
    "rollup-plugin-vue": "^5.1.6",
    "vue-material-design-icons": "^4.6.0",
    "vue-template-compiler": "^2.6.11",
    "yargs": "^15.4.1"
  }
}
