{
  "name": "@dominik.betka/vue-atomic",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drego31/vue-atomic.git"
  },
  "description": "Atomic Design for Vue.js",
  "license": "MIT",
  "author": {
    "name": "dominik.betka",
    "email": "dominik.betka@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Drego31/vue-atomic/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/VueAtomic.vue",
  "module": "dist/",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/VueAtomic.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/VueAtomic.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/VueAtomic.min.js"
  },
  "engines": {
    "node": ">= 7.0"
  },
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "node": "^13.11.0",
    "rollup": "^2.2.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%"
  ]
}
